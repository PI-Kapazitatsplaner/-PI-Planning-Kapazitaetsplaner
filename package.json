{
  "name": "kapazitaetsplaner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require babel-register tests/*.ts --exit",
    "test-dev": "nodemon --watch . --ext ts --exec \"npx mocha -r ts-node/register tests/**/*.test.ts\"",
    "predev": "docker compose -f ./db/docker-compose.yml up -d && prisma migrate dev && prisma db seed",
    "dev": "tsnd --respawn --rs app.ts",
    "prestart": "npx prisma db push && npx prisma generate && npx prisma db seed",
    "start": "ts-node-transpile-only app.ts",
    "docker": "docker build . -t ejs-prisma-ts/poc && docker run -p 8080:3000 -d ejs-prisma-ts/poc && echo \"Container Port mapped to \"http://localhost:8080/\"",
    "prisma:push": "prisma db push",
    "pg": "prisma generate",
    "prisma:generate": "prisma generate",
    "ps": "prisma studio",
    "prisma:studio": "prisma studio"
  },
  "prisma": {
    "schema": "./Prisma/schema.prisma",
    "seed": "ts-node ./Prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PI-Kapazitatsplaner/Kapazitaetsplaner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PI-Kapazitatsplaner/Kapazitaetsplaner/issues"
  },
  "homepage": "https://github.com/PI-Kapazitatsplaner/Kapazitaetsplaner#readme",
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@types/ejs": "^3.1.0",
    "@types/http-errors": "^1.8.2",
    "ejs": "^3.1.6",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.12",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "jest": "^27.4.7",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "prisma": "^3.8.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
